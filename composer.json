{
	"name": "codekandis/entities",
	"description": "`codekandis/entities` is a library providing entity interfaces and classes.",
	"version": "dev-development",
	"keywords": [
		"php",
		"entities",
		"interfaces",
		"classes"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/entities/issues",
		"docs": "https://github.com/codekandis/entities/blob/master/README.md",
		"source": "https://github.com/codekandis/entities/tree/master"
	},
	"require": {
		"php": ">=8.3",
		"ext-json": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^5.0.0",
		"rector/rector": "^0.18.5"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Entities\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Entities\\Build\\": [
				"build/"
			],
			"CodeKandis\\Entities\\Tests\\": [
				"tests/"
			]
		}
	}
}
